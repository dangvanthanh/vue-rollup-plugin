{
  "name": "vue-rollup-plugin",
  "version": "0.0.6",
  "description": "Template for bundle Vue component or plugin",
  "main": "dist/base.umd.js",
  "module": "dist/base.esm.js",
  "unpkg": "dist/base.min.js",
  "style": "dist/base.css",
  "scripts": {
    "serve": "parcel public/index.html",
    "build": "npm run build:browsers && npm run build:es && npm run build:umd",
    "build:browsers": "rollup -c build/rollup.config.browsers.js",
    "build:es": "rollup -c build/rollup.config.es.js",
    "build:umd": "rollup -c build/rollup.config.umd.js",
    "test": "jest",
    "release": "npm run build && npx clean-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangvanthanh/vue-rollup-plugin.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dangvanthanh/vue-rollup-plugin/issues"
  },
  "homepage": "https://github.com/dangvanthanh/vue-rollup-plugin#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@testing-library/dom": "^7.26.5",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/vue": "^5.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "clean-css": "^4.2.3",
    "esbuild": "^0.8.5",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "pascal-case": "^3.1.1",
    "rollup": "^2.33.1",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-esbuild": "^2.5.2",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "files": [
    "dist/base.*.js",
    "src/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "vue-hot-reload-api": "^2.3.4"
  }
}
