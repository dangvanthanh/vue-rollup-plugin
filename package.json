{
  "name": "vue-rollup-plugin",
  "version": "0.1.0",
  "description": "Template for bundle Vue component or plugin for NPM",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "style": "dist/index.css",
  "scripts": {
    "serve": "parcel public/index.html",
    "build": "rimraf dist && rollup -c",
    "test": "jest",
    "release": "npm run build && npx clean-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangvanthanh/vue-rollup-plugin.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dangvanthanh/vue-rollup-plugin/issues"
  },
  "homepage": "https://github.com/dangvanthanh/vue-rollup-plugin#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.12.7",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/vue": "^5.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "clean-css": "^4.2.3",
    "esbuild": "0.8.21",
    "jest": "^26.6.3",
    "pascal-case": "^3.1.2",
    "rimraf": "^3.0.2",
    "rollup": "2.34.2",
    "rollup-plugin-css-only": "^3.0.0",
    "rollup-plugin-esbuild": "^2.6.0",
    "rollup-plugin-filesize": "9.1.0",
    "rollup-plugin-vue": "5.1.9",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ]
